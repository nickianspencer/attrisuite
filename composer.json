{
    "name": "nickspencer/attri-suite",
    "description": "A flexible and powerful PHP library for managing product attributes, with support for nested and dependent attributes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Spencer",
            "email": "mrnicholasspencer@gmail.com",
            "homepage": "https://yourwebsite.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "AttriSuite\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "keywords": [
        "php",
        "pim",
        "attribute management",
        "nested attributes",
        "product information management",
        "ecommerce"
    ],
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }    
}
